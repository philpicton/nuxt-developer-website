{
    "name": "frontend-dev-website",
    "version": "1.0.0",
    "description": "A modern Nuxt website template for frontend developers",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "eslint .",
        "format": "prettier --write \"{app,server,tests,types}/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\"",
        "format:check": "prettier --check \"{app,server,tests,types}/**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md}\"",
        "typecheck": "nuxt typecheck",
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "audit": "bun audit",
        "audit:fix": "bun audit --fix",
        "outdated": "bun outdated",
        "check": "bun run lint && bun run format:check && bun run typecheck && bun run build",
        "test": "NODE_OPTIONS='--no-warnings' node ./node_modules/vitest/vitest.mjs",
        "test:ui": "NODE_OPTIONS='--no-warnings' node ./node_modules/vitest/vitest.mjs --ui"
    },
    "dependencies": {
        "@nuxt/content": "3.8.2",
        "@nuxt/eslint": "1.10.0",
        "@nuxt/fonts": "0.12.1",
        "@nuxt/icon": "2.1.0",
        "@nuxt/image": "1.11.0",
        "@nuxthub/core": "0.9.0",
        "@nuxtjs/color-mode": "3.5.2",
        "@tailwindcss/vite": "4.1.17",
        "@unhead/vue": "2.0.19",
        "better-sqlite3": "12.4.6",
        "eslint": "9.39.1",
        "nuxt": "4.2.1",
        "tailwindcss": "4.1.17",
        "vue": "3.5.25",
        "vue-router": "4.6.3"
    },
    "devDependencies": {
        "@iconify-json/formkit": "1.2.2",
        "@nuxt/test-utils": "3.20.1",
        "@tailwindcss/forms": "0.5.10",
        "@tailwindcss/typography": "0.5.19",
        "@vitejs/plugin-vue": "6.0.1",
        "@vue/test-utils": "2.4.6",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-prettier": "5.5.4",
        "happy-dom": "20.0.10",
        "prettier": "3.6.2",
        "vitest": "4.0.5",
        "wrangler": "4.45.2"
    }
}
